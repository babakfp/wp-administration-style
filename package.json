{
	"name": "wp-administration-style",
	"version": "6.1.1",
	"license": "GPL-3.0",
	"repository": "https://github.com/babakfp/wp-administration-style.git",
	"scripts": {
		"base": "sass static/sass/_main-files/base.sass:static/css/base.css --watch --style compressed --no-source-map",
		"gutenberg": "sass static/sass/_main-files/gutenberg.sass:static/css/gutenberg.css --watch --style compressed --no-source-map",
		"mce": "sass static/sass/_main-files/mce.sass:static/css/mce.css --watch --style compressed --no-source-map",
    "mce-ifr": "sass static/sass/_main-files/mce-ifr.sass:static/css/mce-ifr.css --watch --style compressed --no-source-map",
		"elementor": "sass static/sass/_main-files/elementor.sass:static/css/elementor.css --watch --style compressed --no-source-map",
		"elementor-editor": "sass static/sass/_main-files/elementor-editor.sass:static/css/elementor-editor.css --watch --style compressed --no-source-map",
		"woocommerce": "sass static/sass/_main-files/woocommerce.sass:static/css/woocommerce.css --watch --style compressed --no-source-map",
		"signin": "sass static/sass/_main-files/signin.sass:static/css/signin.css --watch --style compressed --no-source-map",

		"base:once": "sass static/sass/_main-files/base.sass:static/css/base.css --style compressed --no-source-map",
		"gutenberg:once": "sass static/sass/_main-files/gutenberg.sass:static/css/gutenberg.css --style compressed --no-source-map",
		"mce:once": "sass static/sass/_main-files/mce.sass:static/css/mce.css --style compressed --no-source-map",
    "mce-ifr:once": "sass static/sass/_main-files/mce-ifr.sass:static/css/mce-ifr.css --style compressed --no-source-map",
		"elementor:once": "sass static/sass/_main-files/elementor.sass:static/css/elementor.css --style compressed --no-source-map",
		"elementor-editor:once": "sass static/sass/_main-files/elementor-editor.sass:static/css/elementor-editor.css --style compressed --no-source-map",
		"woocommerce:once": "sass static/sass/_main-files/woocommerce.sass:static/css/woocommerce.css --style compressed --no-source-map",
		"signin:once": "sass static/sass/_main-files/signin.sass:static/css/signin.css --style compressed --no-source-map",

		"generate-css": "npm-run-all base:once gutenberg:once mce:once mce-ifr:once elementor:once woocommerce:once signin:once elementor-editor:once"

	}
}
